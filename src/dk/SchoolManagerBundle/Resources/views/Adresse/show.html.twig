{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Adresse</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Telfixe</th>
                <td>{{ entity.telfixe }}</td>
            </tr>
            <tr>
                <th>Adresse1</th>
                <td>{{ entity.adresse1 }}</td>
            </tr>
            <tr>
                <th>Adresse2</th>
                <td>{{ entity.adresse2 }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ entity.codepostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ entity.ville }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('adresse') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('adresse_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
