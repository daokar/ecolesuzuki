<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tarif" table="tarif">
    <id name="idtarif" type="integer" column="IDTARIF">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typetarif" type="string" column="TYPETARIF" length="55" nullable="false"/>
    <field name="montanttarif" type="float" column="MONTANTTARIF" precision="5" scale="2" nullable="false"/>
    <field name="datetarif" type="date" column="DATETARIF" nullable="true"/>
  </entity>
</doctrine-mapping>
