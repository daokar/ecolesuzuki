<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Adresse" table="adresse">
    <id name="idadresse" type="integer" column="IDADRESSE">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="telfixe" type="string" column="TELFIXE" length="10" nullable="true"/>
    <field name="adresse1" type="string" column="ADRESSE1" length="55" nullable="true"/>
    <field name="adresse2" type="string" column="ADRESSE2" length="55" nullable="true"/>
    <field name="codepostal" type="string" column="CODEPOSTAL" length="5" nullable="true"/>
    <field name="ville" type="string" column="VILLE" length="55" nullable="true"/>
  </entity>
</doctrine-mapping>
